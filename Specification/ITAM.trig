    rdfs:isDefinedBy itam:.    

  concept:Laptop a skos:Concept ;
    skos:inScheme scheme:ITAM;  
    skos:prefLabel 'laptop'@en,
                   'laptop'@nl;
    skos:definition 'A portable computer that is small and lightweight, designed for mobile use.'@en,
                    'Een draagbare computer die klein en licht is, ontworpen voor mobiel gebruik.'@nl;
    rdfs:isDefinedBy itam:.

  concept:LaptopRecord a skos:Concept ;
    skos:inScheme scheme:ITAM;
    skos:prefLabel 'record: laptop'@en,
                   'registratie: laptop'@nl;
    skos:definition 'A record of a laptop.'@en,
                    'Een registratie van een laptop.'@nl;
    rdfs:isDefinedBy itam:.

 concept:License a skos:Concept;
    skos:inScheme scheme:ITAM; 
    skos:prefLabel 'license'@en,
                   'licentie'@nl;
    skos:definition 'Legal agreement between two parties, the licensor and the licensee, as to the terms and conditions for the use or transfer of an intellectual property right from the licensor to the licensee.'@en,
                    'Juridische overeenkomst tussen twee partijen, de licentiegever en de licentienemer, met betrekking tot de voorwaarden voor het gebruik of de overdracht van een intellectueel eigendomsrecht van de licentiegever aan de licentienemer.'@nl;
    rdfs:isDefinedBy itam:.

  concept:Location a skos:Concept ;
    skos:inScheme scheme:ITAM;  
    skos:prefLabel 'location'@en,
                   'locatie'@nl;
    skos:definition 'The location of the IT asset. A location can be an identifiable geographic place (ISO 19112), but it can also be a non-geographic place such as a directory, row, or column. As such, there are numerous ways in which location can be expressed, such as by a coordinate, address, landmark, and so forth.'@en,
                    'De locatie van het IT-bedrijfsmiddel. Een locatie kan een identificeerbare geografische plaats zijn (ISO 19112), maar het kan ook een niet-geografische plaats zijn, zoals een directory, rij of kolom. Als zodanig zijn er talloze manieren waarop locatie kan worden uitgedrukt, zoals door een coördinaat, adres, oriëntatiepunt, enzovoort.'@nl;
    rdfs:isDefinedBy itam:.

  concept:LicenseRecord a skos:Concept;
    skos:inScheme scheme:ITAM;  
    skos:prefLabel 'record: license'@en,
                   'registratie: licentie'@nl;
    skos:definition 'A record of a license.'@en,
                    'Een registratie van een licentie.'@nl;
    rdfs:isDefinedBy itam:.    

  concept:Microphone a skos:Concept ;
    skos:inScheme scheme:ITAM;  
    skos:prefLabel 'microphone'@en,
                   'microfoon'@nl;
    skos:definition 'A device that captures sound waves and converts them into electrical signals for recording, communication, or amplification.'@en,
                    'Een apparaat dat geluidsgolven opvangt en omzet in elektrische signalen voor opname, communicatie of versterking.'@nl;
    rdfs:isDefinedBy itam:.

  concept:MicrophoneRecord a skos:Concept ;
    skos:inScheme scheme:ITAM;  
    skos:prefLabel 'record: microphone'@en,
