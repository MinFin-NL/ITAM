                prov:qualifiedUsage [
                    a prov:Usage;
                    prov:entity [
                        a prov:Entity;
                        prov:specializationOf asset:SourceApplication;
                        prov:wasGeneratedAtTime "2025-05-01T10:00:00"^^xsd:dateTime
