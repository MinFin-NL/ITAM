  itam:ObjectRecord a owl:Class;
    rdfs:subClassOf foaf:Document;
    owl:equivalentClass [a owl:Restriction;
              owl:onProperty foaf:primaryTopic;
              owl:allValuesFrom itam:Object;
              owl:someValuesFrom itam:Object];
    skos:prefLabel 'object record'@en,
                   'objectregistratie'@nl;
    skos:definition 'A record of an object that captures the state and characteristics of that object.'@en,
                    'Een registratie van een object waarin de staat en kenmerken van het object worden vastgelegd.'@nl;
    rdfs:seeAlso concept:ObjectRecord;
    rdfs:isDefinedBy itam:.

  ### Classes: Asset

  itam:Asset a owl:Class;
    rdfs:subClassOf itam:Object;
    skos:prefLabel 'asset'@en,
                   'bedrijfsmiddel'@nl;
    skos:definition 'Item, thing or entity that has potential or actual value to an organization.'@en,
                    'Product, ding of entiteit die potentiële of daadwerkelijke waarde heeft voor een organisatie.'@nl;
    rdfs:seeAlso concept:Asset;
    rdfs:isDefinedBy itam:.

  itam:AssetRecord a owl:Class;
    rdfs:subClassOf itam:ObjectRecord;
    owl:equivalentClass [a owl:Restriction;
              owl:onProperty foaf:primaryTopic;
              owl:allValuesFrom itam:Asset;
              owl:someValuesFrom itam:Asset];
    skos:prefLabel 'asset record'@en,
                   'bedrijfsmiddelregistratie'@nl;
    skos:definition 'A record of an asset that captures its state and its characteristics.'@en,
                    'Een registratie van een bedrijfsmiddel waarin de staat en kenmerken ervan worden vastgelegd.'@nl;
    rdfs:seeAlso concept:AssetRecord;
    rdfs:isDefinedBy itam:.

  itam:InformationTechnologyAsset a owl:Class;
    rdfs:subClassOf itam:Asset;
    owl:equivalentClass [a owl:Class;
                owl:unionOf (
