  concept:Firewall a skos:Concept ;
    skos:inScheme scheme:ITAM;  
    skos:prefLabel 'firewall'@en,
                   'firewall'@nl;
    skos:definition 'A security device that monitors and controls incoming and outgoing network traffic.'@en,
