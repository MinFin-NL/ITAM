    rdfs:isDefinedBy itam:.

  concept:HardwareSecurityModule a skos:Concept ;
    skos:inScheme scheme:ITAM;  
    skos:prefLabel 'hardware security module'@en,
                   'hardwarebeveiligingsmodule'@nl;
    skos:definition 'A hardware security module (HSM) is a physical computing device that safeguards and manages secrets (most importantly digital keys), and performs encryption and decryption functions for digital signatures, strong authentication and other cryptographic functions.'@en,
                    'Een hardwarebeveiligingsmodule (HSM) is een fysiek computerapparaat dat geheimen (met name digitale sleutels) beveiligt en beheert, en encryptie- en decryptiefuncties uitvoert voor digitale handtekeningen, sterke authenticatie en andere cryptografische functies.'@nl;
    rdfs:isDefinedBy itam:.

  concept:HardwareSecurityModuleRecord a skos:Concept ;
    skos:inScheme scheme:ITAM;  
    skos:prefLabel 'record: hardware security module'@en,
                   'registratie: hardwarebeveiligingsmodule'@nl;
    skos:definition 'A record of a hardware security module.'@en,
                    'Een registratie van een hardwarebeveiligingsmodule.'@nl;
    rdfs:isDefinedBy itam:.

  concept:HardwareSessionBorderController a skos:Concept;
    skos:inScheme scheme:ITAM;  
    skos:prefLabel 'session border controller'@en ,
                   'session border controller'@nl ;
    skos:altLabel 'SBC'@en ,
                  'SBC'@nl ;
    skos:definition 'A session border controller (SBC) is a physical network device that secures and manages VoIP and multimedia traffic at the edge of a network.'@en,
                    'Een Session Border Controller (SBC) is een fysiek netwerkapparaat dat VoIP- en multimediaverkeer aan de rand van een netwerk beveiligt en beheert.'@nl;
    rdfs:isDefinedBy itam:.

