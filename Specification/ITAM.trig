    rdfs:isDefinedBy itam:.

  concept:HardwareToken a skos:Concept ;
    skos:inScheme scheme:ITAM;  
    skos:prefLabel 'hardware token'@en,
                   'hardwaretoken'@nl;
    skos:definition 'A device that produces a token to gain access to an electronically restricted resource.'@en,
                    'Een apparaat dat een token produceert waarmee toegang verkregen kan worden tot een elektronisch afgeschermde bron.'@nl;
    rdfs:isDefinedBy itam:.

