    rdfs:isDefinedBy itam:.

  concept:Desktop a skos:Concept ;
    skos:inScheme scheme:ITAM;  
    skos:prefLabel 'desktop'@en,
                   'desktop'@nl;
    skos:definition 'A personal computer designed to be used at a desk or workstation.'@en,
                    'Een personal computer (PC) die is ontworpen voor gebruik op een bureau of werkstation.'@nl;
    rdfs:isDefinedBy itam:.

  concept:DesktopRecord a skos:Concept ;
