  ### Shapes

  ## Asset
  shp:Asset a sh:NodeShape;
     sh:target [
       a sh:SPARQLTarget;
       sh:prefixes itam:;
       sh:select """
          SELECT ?this
          WHERE {
             ?this rdf:type/rdfs:subClassOf* itam:Asset .
             FILTER NOT EXISTS {?this dct:isReplacedBy ?record}
          }
          """;
    ] ;
    sh:property shp:Asset_hasRecord,
                shp:hasPrefLabel,
                shp:Asset_hasAssetID;
    rdfs:isDefinedBy itam:.

  shp:Asset_hasRecord a sh:PropertyShape;
    sh:path [sh:inversePath foaf:primaryTopic];
    sh:minCount 1;
    sh:class itam:AssetRecord;
    sh:severity sh:Violation;
    sh:message "Iedere instantie van itam:Asset of subtype ervan dient minimaal 1 record te hebben die ernaar verwijst."@nl;
    sh:message "Every instance of itam:Asset or subtype should have at least 1 record referring to it."@en;
    rdfs:isDefinedBy itam:.

  shp:Asset_hasAssetID a sh:PropertyShape;
    sh:path itam:assetID;
    sh:minCount 1;
    sh:maxCount 1;
    sh:uniqueLang true;
    sh:severity sh:Violation;
    sh:message "Iedere instantie van itam:Asset heeft exact 1 itam:assetID om het asset mee te identificeren."@nl;
    sh:message "Every itam:Asset instance has exactly 1 itam:assetID which is used to identify the asset."@en;
    rdfs:isDefinedBy itam:.

  ## AssetRecord
  shp:CurrentAssetRecord a sh:NodeShape;
    sh:target [
       a sh:SPARQLTarget;
       sh:prefixes itam:;
       sh:select """
          SELECT ?this
          WHERE {
             ?this rdf:type/rdfs:subClassOf* itam:AssetRecord .
             FILTER NOT EXISTS {?this dct:isReplacedBy ?record}
             FILTER EXISTS {?this itam:hasAccrualStatus itam:Closed}
          }
          """;
    ];
    sh:property shp:hasPrefLabel,
                shp:AssetRecord_hasPrimaryTopic,
                shp:AssetRecord_hasAssetStatus;
    rdfs:isDefinedBy itam:.

  shp:AssetRecord_hasPrimaryTopic a sh:PropertyShape;
    sh:path foaf:primaryTopic;
    sh:class itam:Asset;
    sh:minCount 1;
    sh:maxCount 1;
    sh:severity sh:Violation;
    sh:message "Ieder record moet precies 1 asset als primary topic hebben."@nl;
    sh:message "Every record needs to have exactly 1 asset as primary topic."@en;
    rdfs:isDefinedBy itam:.

  shp:AssetRecord_hasAssetStatus a sh:PropertyShape;
    sh:path itam:hasAssetStatus;
    sh:minCount 1;
    sh:maxCount 1;
    sh:class skos:Concept;
    sh:severity sh:Violation;
    sh:message "Ieder record moet precies 1 Asset Status hebben, deze status moet een concept zijn."@nl;
    sh:message "Every record needs to have exactly 1 Asset Status, this status has to be a concept."@en;
    rdfs:isDefinedBy itam:.

  shp:AssetRecord a sh:NodeShape;
    sh:target [
       a sh:SPARQLTarget;
       sh:prefixes itam:;
       sh:select """
          SELECT ?this
          WHERE {
             ?this rdf:type/rdfs:subClassOf* itam:AssetRecord .
          }
          """;
    ];
    sh:property shp:AssetRecord_replaces,
                shp:AssetRecord_isReplacedBy;
    rdfs:isDefinedBy itam:.

  shp:AssetRecord_isReplacedBy a sh:PropertyShape; 
    sh:path dct:isReplacedBy;
    sh:class itam:AssetRecord;
    sh:maxCount 1;
    sh:severity sh:Violation;
    sh:message "Ieder record wordt vervangen door maximaal 1 record."@nl;
    sh:message "Each record is replaced by a maximum of 1 record."@en;
    rdfs:isDefinedBy itam:.

  shp:AssetRecord_replaces a sh:PropertyShape;
    sh:path dct:replaces;
    sh:class itam:AssetRecord;
    sh:maxCount 1;
    sh:severity sh:Violation;
    sh:message "Ieder record vervangt maximaal 1 record."@nl;
    sh:message "Each record replaces a maximum of 1 record."@en;
    rdfs:isDefinedBy itam:.

 ## ExternalIdentifier
 shp:ExternalIdentifier a sh:NodeShape;
    sh:targetClass itam:ExternalIdentifier;
    sh:property shp:ExternalIdentifier_hasValue;
    rdfs:isDefinedBy itam:.

  shp:ExternalIdentifier_hasValue a sh:PropertyShape;
    sh:path rdf:value;
    sh:minCount 1;
    sh:severity sh:Violation;
    sh:message "Elke itam:ExternalIdentifier heeft minimaal 1 waarde voor rdf:value."@nl;
    sh:message "Every itam:ExternalIdentifier has at least 1 rdf:value."@en;
    rdfs:isDefinedBy itam:.
 
  ## Generic PropertyShapes
  shp:hasPrefLabel a sh:PropertyShape;
    sh:path skos:prefLabel;
    sh:minCount 1;
    sh:uniqueLang true;
    sh:severity sh:Violation;
    sh:message "Object dient minimaal 1 prefLabel te hebben, en maximaal 1 per taal."@nl;
    sh:message "Object must have at least 1 prefLabel, and at most 1 per language."@en;
    rdfs:isDefinedBy itam:.
}